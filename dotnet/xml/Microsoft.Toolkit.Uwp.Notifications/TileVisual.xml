    <Member MemberName="AddImageQuery">
      <MemberSignature Language="C#" Value="public Nullable&lt;bool&gt; AddImageQuery { get; set; }" />
      <MemberSignature Language="ILAsm" Value=".property instance valuetype System.Nullable`1&lt;bool&gt; AddImageQuery" />
